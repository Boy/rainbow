				<string name="gesture_label">
					ジェスチャー
				</string>
				<line_editor label="チャットを開始するには、ここをクリックします。" name="Chat Editor"/>
				<flyout_button label="発言" name="Say" tool_tip="（Enter）">
					<flyout_button_item name="shout_item">
						叫ぶ
					</flyout_button_item>
					<flyout_button_item name="say_item">
						発言
					</flyout_button_item>
					<flyout_button_item name="whisper_item">
						ささやく
					</flyout_button_item>
				</flyout_button>
			</panel>
		</layout_panel>
