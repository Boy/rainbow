<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel name="script panel">
	<text_editor type="string" length="1" name="Script Editor">
		ローディング．．．
	</text_editor>
	<button label="保存" label_selected="保存" name="Save_btn" />
