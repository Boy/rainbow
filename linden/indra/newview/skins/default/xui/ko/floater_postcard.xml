<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater name="Postcard" title="이메일로 스냅샷 보내기">
	<text name="to_label">
		수신인 이메일:
	</text>
	<text name="from_label">
		이메일:
	</text>
	<text name="name_label">
		이름:
	</text>
	<text name="subject_label">
		제목:
	</text>
	<line_editor label="여기에 제목을 입력합니다." name="subject_form" />
	<text name="msg_label">
		메시지:
	</text>
	<text_editor name="msg_form">
		여기에 메시지를 입력합니다.
	</text_editor>
	<check_box label="웹에 게시" name="allow_publish_check"
	     tool_tip="이 엽서를 웹에 게시." />
	<check_box label="성인용 컨텐츠" name="mature_check"
	     tool_tip="이 엽서에는 성인 전용 컨텐츠가 포함되어 있습니다." />
	<button label="?" name="publish_help_btn" />
	<text name="fine_print">
		수령인이 세컨드라이프에 가입을 하면 추천 보너스를 얻게 됩니다.
	</text>
	<button label="취소" name="cancel_btn" />
	<button label="보내기" name="send_btn" />
	<text name="default_subject">
		Second Life로부터의 엽서
	</text>
	<text name="default_message">
		여기를 체크하여 주십시요.
	</text>
	<string name="upload_message">
		보내는 중...
	</string>
</floater>
