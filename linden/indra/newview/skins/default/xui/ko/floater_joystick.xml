	</check_box>
	<text name="XScale">
		X 크기
	</text>
	<text name="YScale">
		Y 크기
	</text>
	<text name="ZScale">
		Z 크기
	</text>
	<text name="PitchScale">
		피치 크기
	</text>
	<text name="YawScale">
		요 크기
	</text>
	<text name="RollScale">
		롤 크기
	</text>
	<text name="XDeadZone">
		X 사각지대
	</text>
	<text name="YDeadZone">
		Y 사각지대
	</text>
	<text name="ZDeadZone">
		Z 사각지대
	</text>
	<text name="PitchDeadZone">
		피치 사각지대
	</text>
	<text name="YawDeadZone">
		요 사각지대
	</text>
	<text name="RollDeadZone">
		롤 사각지대
	</text>
	<text name="Feathering">
		부드러움 효과
	</text>
	<text name="ZoomScale2">
		확대/축소 크기
	</text>
	<text left="0" name="ZoomDeadZone" width="140">
		확대/축소 사각지대
	</text>
	<button label="SpaceNavigator 기본값" name="SpaceNavigatorDefaults" />
	<string name="JoystickMonitor">
		조이스틱 검사
	</string>
	<string name="Axis">
		축 [NUM]
	</string>
	<string name="NoDevice">
		검색된 장치가 없습니다.
	</string>
</floater>
