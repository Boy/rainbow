<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater name="Joystick" title="조이스틱 설정">
	<check_box name="enable_joystick">
		죠이스틱 활성:
	</check_box>
	<spinner label="X축 매핑" name="JoystickAxis1" />
	<spinner label="Y축 매핑" name="JoystickAxis2" />
	<spinner label="Z축 매핑" name="JoystickAxis0" />
	<spinner label="피치 매핑" name="JoystickAxis4" />
	<spinner label="요 매핑" name="JoystickAxis5" />
	<spinner label="롤 매핑" name="JoystickAxis3" />
	<spinner label="확대/축소 매핑" name="JoystickAxis6" />
	<check_box label="직접 확대/축소" name="ZoomDirect" />
	<check_box label="3D 커서" name="Cursor3D" />
