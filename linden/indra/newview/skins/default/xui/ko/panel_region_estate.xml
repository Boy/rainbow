<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel label="사유지" name="Estate">
	<text name="estate_help_text">
		이 탭에서 설정을 변경하면 사유지 내 모든 지역에 적용됩니다.
	</text>
	<text name="estate_text">
		사유지:
	</text>
	<text name="estate_name">
		(알 수 없음)
	</text>
	<text name="owner_text">
		소유자:
	</text>
	<text name="estate_owner">
		(알 수 없음)
	</text>
	<text name="Only Allow">
		출입 제한 대상:
	</text>
	<check_box label="파일에 결제 정보가 있는 사용자" name="limit_payment"
	     tool_tip="확인되지 않은 사용자는 출입이 차단됩니다." />
	<check_box label="성인 인증을 완료한 사용자" name="limit_age_verified"
	     tool_tip="성인 인증을 완료하지 않은 사용자는 출입이 차단됩니다. 자세한 사항은 support.secondlife.com을 참조하십시오." />
	<check_box label="음성 채팅 허용" name="voice_chat_check" />
	<button label="?" name="voice_chat_help" />
	<text name="abuse_email_text">
		악용 신고 이메일 주소:
	</text>
	<string name="email_unsupported">
		기능이 지원되지 않습니다.
	</string>
	<button label="?" name="abuse_email_address_help" />
	<text name="estate_manager_label">
		사유지 관리자:
	</text>
	<button label="?" name="estate_manager_help" />
	<button label="추가..." name="add_estate_manager_btn" />
	<button label="제거…" name="remove_estate_manager_btn" />
	<check_box label="표준시각 사용" name="use_global_time_check" />
	<button label="?" name="use_global_time_help" />
	<check_box label="태양 고정" name="fixed_sun_check" />
	<button label="?" name="fixed_sun_help" />
	<slider label="단계" name="sun_hour_slider" />
	<check_box label="일반 공개 허용" name="externally_visible_check" />
	<button label="?" name="externally_visible_help" />
	<check_box label="직접 텔레포트 허용" name="allow_direct_teleport" />
	<button label="?" name="allow_direct_teleport_help" />
	<text name="region_text_lbl">
		지불 상태별로 출입 거부:
	</text>
	<check_box label="결제수단 미등록자 출입 거부" name="deny_anonymous" />
	<check_box label="결제수단 등록자 출입 거부" name="deny_identified" />
	<check_box label="결제수단 사용자 출입 거부" name="deny_transacted" />
	<button label="적용" name="apply_btn" />
	<text name="allow_resident_label">
		허용된 주민:
	</text>
	<button label="?" name="allow_resident_help" />
	<button label="추가..." name="add_allowed_avatar_btn" />
	<button label="제거…" name="remove_allowed_avatar_btn" />
	<text name="allow_group_label">
		허용된 그룹:
	</text>
	<button label="?" name="allow_group_help" />
	<button label="추가..." name="add_allowed_group_btn" />
	<button label="제거…" name="remove_allowed_group_btn" />
	<text name="ban_resident_label">
		출입금지된 주민:
	</text>
	<button label="?" name="ban_resident_help" />
	<button label="추가..." name="add_banned_avatar_btn" />
	<button label="제거…" name="remove_banned_avatar_btn" />
	<button label="사유지에 메시지 보내기…" name="message_estate_btn" />
	<button label="추방하기…" name="kick_user_from_estate_btn" />
</panel>
