<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel name="toolbar">
	<string name="Redock Windows">
		창 합치기
	</string>
	<layout_stack name="toolbar_stack">
		<button label="통신" name="communicate_btn"
		     tool_tip="친구 및 그룹과 통신합니다." />
		<button label="" name="chat_btn" tool_tip="채팅 표시줄 표시 (입력)" />
		<button label="친구" name="friends_btn"
		     tool_tip="친구를 찾아 이야기합니다." />
		<button label="비행" label_selected="비행 중지" name="fly_btn"
		     tool_tip="비행 시작. 위, 아래로 비행하려면 E/C 또는 PgUp/PgDn을 사용하십시오." />
		<button label="스냅샷" name="snapshot_btn"
		     tool_tip="디스크 또는 인벤토리에 스크린샷 저장." />
		<button label="검색" name="directory_btn"
		     tool_tip="장소, 이벤트, 사람, 더 자세히 등을 검색할 수 있습니다." />
		<button label="만들기" name="build_btn" tool_tip="새 오브젝트를 만듭니다." />
		<button label="미니지도" name="radar_btn"
		     tool_tip="내 주변 영역의 지도 (Ctrl-Shitf-M)" />
		<button label="지도" name="map_btn" tool_tip="월드 지도 (Ctrl-M)" />
		<button label="인벤토리" name="inventory_btn" tool_tip="아이템. (Ctrl-I)" />
	</layout_stack>
</panel>
