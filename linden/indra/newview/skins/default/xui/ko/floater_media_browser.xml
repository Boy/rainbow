<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater name="floater_about" title="미디어 브라우저">
	<layout_stack name="stack1">
		<layout_panel name="nav_controls">
			<button label="뒤로" name="back" />
			<button label="앞으로" name="forward" />
			<button label="다시 로드" name="reload" />
			<button label="이동" name="go" />
		</layout_panel>
		<layout_panel name="parcel_owner_controls">
			<button label="현재 URL을 구획으로 보내기" name="assign" />
		</layout_panel>
		<layout_panel name="external_controls">
			<button label="웹 브라우저에서 열기" name="open_browser" />
			<check_box label="항상 웹 브라우저에서 열기" name="open_always" />
			<button label="닫기" name="close" />
		</layout_panel>
	</layout_stack>
</floater>
